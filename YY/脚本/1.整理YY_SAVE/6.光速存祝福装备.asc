dim @a
dim @x, @y
dim @z,@npc,@oz
let @a,=,0
let @oz,=,-2


label 开始
ifpos 19,33,+2
walkpos 19,33 
set 交易,1
set 原地保护,1
ifitem 空位,!=,15,存
delay 500
lclick 66,391 
goto 开始

label 存
walkpos 18,34 
set 交易,0
let @x,=,@[座标X]
let @y,=,@[座标Y]
let @z,=,@z

if @a,=,0,小仓库
if @a,=,1,大仓1
if @a,=,2,大仓2
end

label 小仓库
let @npc,=,291
button ESC
say hi
waitdlg ?,0,10,重登
button 1
delay 100
ifdlg 超过,1,大仓1
waitdlg ?,0,10,重登
call 存道具
delay 100
goto 开始


label 大仓1
let @oz,=,-2
let @z,=,-1
let @npc,=,312
button ESC
let @a,=,1
say hi
waitdlg ?,0,10,重登
button 3
waitdlg ?,0,10,重登
cls
delay 10
button 1
delay 100
ifsay 1-10,已满,结束
waitdlg ?,0,10,重登
call 存道具

delay 100
goto 开始

label 结束
ifpos 19,33,+2
walkpos 19,33 
set 交易,1
ifitem 空位,=,0,+4
delay 500
lclick 66,391 
goto 结束
set 交易,1
end


label 存道具
let @z,=,@[寻找道具.祝福]
'print @z
'print @oz
if @z,=,-1,没有了
if @z,=,@oz,大仓1
let @oz,=,@z
'print @z
'print @oz
let @z,+,9
button @z,@x,@y,@npc
delay 100
goto 存道具
label 没有了
return

label 重登
log 2
set 断线重登,1
set 自动登陆,1
waitdlg 上一次,?,10,重登
goto 开始